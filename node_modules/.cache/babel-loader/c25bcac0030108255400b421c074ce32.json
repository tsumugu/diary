</div>\n        <div><button @change=\"onSubmit\">送信</button></div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport firebase from 'firebase'\nvar database = firebase.database();\n\nexport default {\n  name: \"regist\",\n  data () {\n    return {\n      isSignIn: null,\n      userInfo: null,\n      isNowLoading: true,\n      previewImageList: [],\n      placeList: [\n        {id:\"thisisid-aaaa\", name:\"Tokyo\"},\n        {id:\"thisisid-bbbb\", name:\"Kyoto\"},\n        {id:\"thisisid-cccc\", name:\"Hokkaido\"}\n      ],\n      friendsList: [\n        {id:\"thisisid-dddd\", name:\"Taro\"},\n        {id:\"thisisid-eeee\", name:\"Jiro\"},\n        {id:\"thisisid-ffff\", name:\"Saburo\"}\n      ],\n      when: null,\n      where: null,\n      who: null,\n      what: null,\n      photo: null,\n    }\n  },\n  watch: {\n    isSignIn: function (newValue, oldValue) {\n      //alert(this.isSignIn)\n    },\n    where: function () {\n      console.log(this.where)\n    },\n    who: function () {\n      console.log(this.who)\n    }\n  },\n  methods: {\n    onFileChange(e) {\n      const files = e.target.files || e.dataTransfer.files;\n      Array.from(files).forEach(file => {\n        this.createPreviewImage(file)\n      });\n      //this.createPreviewImage(files)\n    },\n    createPreviewImage(file) {\n      const reader = new FileReader();\n      reader.onload = e => {\n        this.previewImageList.push(e.target.result);\n        console.log(this.previewImageList);\n      };\n      reader.readAsDataURL(file);\n    },\n    onSubmit() {\n      // submit処理\n    }\n  },\n  mounted() {\n    const _this = this\n    firebase.auth().onAuthStateChanged(user => {\n      _this.isSignIn = (user != null)\n      _this.userInfo = user\n      _this.isNowLoading = false\n    })\n  }\n}\n</script>\n\n<style scoped lang=\"scss\">\n.regist {\n  width: 100%;\n  height: 100%;\n  &__loading {\n    width: 100%;\n    height: 100%;\n  }\n  &__body {\n    width: 100%;\n    height: 100%;\n    &__signined {\n      width: 100%;\n      height: 100%;\n    }\n  }\n}\n.imgPreview > img {\n  width: 100px;\n}\n</style>"],"sourceRoot":"src/views"}]}