iv>\n    </div>\n  </div>\n</template>\n\n<script>\nimport firebase from 'firebase'\n\nimport Welcome from '@/components/Welcome.vue'\nimport TimeLine from '@/components/TimeLine.vue'\n\nexport default {\n  name: 'Home',\n  components: {\n    Welcome,\n    TimeLine\n  },\n  data () {\n    return {\n      isSignIn: null,\n      userInfo: null,\n      isNowLoading: true\n    }\n  },\n  mounted() {\n    //ログイン状況を確認\n    const _this = this\n    firebase.auth().onAuthStateChanged(user => {\n      _this.isSignIn = (user != null)\n      _this.userInfo = user\n      _this.isNowLoading = false\n    })\n  }\n}\n</script>\n\n<style lang=\"scss\">\n.home {\n  width: 100%;\n  height: 100%;\n  &__loading {\n    width: 100%;\n    height: 100%;\n  }\n  &__body {\n    width: 100%;\n    height: 100%;\n    &__signined {\n      width: 100%;\n      height: 100%;\n    }\n    &__welcome {\n      width: 100%;\n      height: 100%;\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/views"}]}